{
  "name": "Synapse",
  "version": "3.0.1",
  "description": "Commercial Bank IT-engineer's integrative tool",
  "main": "synapse",
  "scripts": {
    "dev:api": "nodemon --inspect synapse --development",
    "dev:server": "node synapse --development --dev-server --port=3000 --base-url=http://localhost",
    "build": "cross-env NODE_ENV=production webpack --mode production --hide-modules --display-error-details --config ./client_source/webpack.config.js",
    "postinstall": "@if not exist \"node_modules\\synapse\" mklink /J \"node_modules\\synapse\" \"core\""
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "client_source/*",
      "client/*",
      "tasks/*"
    ],
    "events": {
      "restart": ""
    },
    "watch": [
      "./*"
    ]
  },
  "dependencies": {
    "activedirectory": "0.7.2",
    "axios": "0.18.1",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "cron": "1.4.1",
    "cron-parser": "2.6.0",
    "docxtemplater": "3.9.0",
    "emailjs": "2.2.0",
    "express": "4.16.3",
    "express-ntlm": "2.3.0",
    "formidable": "1.2.1",
    "iconv-lite": "0.4.24",
    "ini": "1.3.5",
    "moment": "2.22.2",
    "moment-precise-range-plugin": "1.3.0",
    "morgan": "1.9.1",
    "oracledb": "github:oracle/node-oracledb#v3.1.0",
    "request": "2.88.0",
    "sleep": "5.2.3",
    "soap": "0.25.0",
    "sqlite3": "^4.1.0",
    "winax": "1.12.0",
    "xlsx-template": "0.4.0",
    "xmlbuilder": "10.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@vue/babel-preset-app": "3.0.4",
    "babel-loader": "8.0.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-polyfill": "6.26.0",
    "brace": "0.11.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eventsource-polyfill": "0.9.6",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "json-loader": "0.5.7",
    "node-sass": "4.11.0",
    "nodemon": "1.18.9",
    "pug": "2.0.3",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "7.1.0",
    "stylus": "0.54.5",
    "stylus-loader": "*",
    "url-loader": "1.1.1",
    "vue": "2.5.22",
    "vue-loader": "15.5.1",
    "vue-router": "3.0.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.22",
    "vuetify": "1.5.13",
    "vuex": "3.1.0",
    "webpack": "4.20.2",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.2.0",
    "webpack-hot-middleware": "2.23.1",
    "webpackbar": "3.1.5"
  },
  "author": "Denis Bogachev <denesey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yenesey/Synapse.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yenesey/Synapse/issues"
  },
  "homepage": "https://github.com/yenesey/Synapse#readme"
}
