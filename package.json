{
  "name": "synapse",
  "version": "3.0.1",
  "description": "Commercial Bank IT-engineer's integrative tool",
  "main": "synapse",
  "scripts": {
    "dev:api": "nodemon --inspect synapse --development",
    "dev:server": "node synapse --development --dev-server --port=3000 --base-url=http://localhost",
    "build": "cross-env NODE_ENV=production webpack --mode production --hide-modules --display-error-details --config ./client_source/webpack.config.js",
    "postinstall": "@if not exist \"node_modules\\synapse\" mklink /J \"node_modules\\synapse\" \"core\""
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "client_source/*",
      "client/*",
      "tasks/*"
    ],
    "events": {
      "restart": ""
    },
    "watch": [
      "./*"
    ]
  },
  "dependencies": {
    "activedirectory": "0.7.2",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cron": "^1.7.2",
    "cron-parser": "^2.13.0",
    "docxtemplater": "^3.14.3",
    "emailjs": "2.2.0",
    "formidable": "1.2.1",
    "iconv-lite": "^0.5.0",
    "ini": "1.3.5",
    "moment": "^2.24.0",
    "moment-precise-range-plugin": "1.3.0",
    "morgan": "1.9.1",
    "oracledb": "^4.0.1",
    "request": "2.88.0",
    "sleep": "^6.1.0",
    "soap": "^0.29.0",
    "sqlite3": "^4.1.0",
    "winax": "^1.12.0",
    "xlsx-template": "^0.5.0",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@vue/babel-preset-app": "^3.11.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-polyfill": "6.26.0",
    "brace": "0.11.1",
    "css-loader": "^3.2.0",
    "eventsource-polyfill": "0.9.6",
    "express": "^4.17.1",
    "express-ntlm": "^2.4.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "json-loader": "0.5.7",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "pug": "^2.0.4",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "^8.0.0",
    "url-loader": "^2.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.1.3",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^2.1.1",
    "vuetify-loader": "^1.3.0",
    "vuex": "^3.1.1",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpackbar": "^4.0.0"
  },
  "author": "Denis Bogachev <denesey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yenesey/Synapse.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yenesey/Synapse/issues"
  },
  "homepage": "https://github.com/yenesey/Synapse#readme"
}
